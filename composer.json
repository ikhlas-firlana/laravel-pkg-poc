{
    "name": "ifirlana/poc",
    "description": "proof of concept",
    "keywords": [
        "ifirlana",
        "poc"
    ],
    "homepage": "https://github.com/ifirlana/poc",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ikhlas firlana",
            "email": "ikhlas@firlana.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ifirlana\\Poc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ifirlana\\Poc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ifirlana\\Poc\\PocServiceProvider"
            ],
            "aliases": {
                "Poc": "Ifirlana\\Poc\\PocFacade",
                "Calculator": "Ifirlana\\Poc\\Facades\\Calculator"
            }
        }
    }
}
